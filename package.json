{
  "name": "do-devops",
  "version": "0.6.1",
  "description": "Devops commands for your nodejs projects",
  "main": "./bin/index.js",
  "typings": "./bin/index.d.ts",
  "bin": {
    "do": "./bin/do.js"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "clean": "rimraf src/**/*.js && rimraf bin",
    "test": "mocha -r ts-node/register test/**/*-spec.ts",
    "build": "tsc && chmod +x bin/do.js",
    "watch": "run-p watch:*",
    "watch:build": "tsc -w",
    "watch:autoindex": "yarn do autoindex -w",
    "do": "./bin/do.js",
    "visualize": "yarn madge src --extensions ts -i /tmp/dep-ts-visualize.png && open /tmp/dep-ts-visualize.png",
    "visualize:js": "yarn madge bin --extensions js -i /tmp/dep-js-visualize.png && open /tmp/dep-js-visualize.png",
    "circular": "yarn madge src --extensions ts --circular",
    "orphans": "yarn madge src --extensions ts --orphans",
    "leaves": "yarn madge src --extensions ts --leaves",
    "warnings": "yarn madge src --extensions ts --warnings",
    "summary": "yarn madge src --extensions ts --summary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inocan-group/devops.git"
  },
  "keywords": [
    "devops",
    "nodejs",
    "serverless"
  ],
  "author": "Ken Snyder",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inocan-group/devops/issues"
  },
  "homepage": "https://github.com/inocan-group/devops#readme",
  "dependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "ast-types": "^0.14.2",
    "async-shelljs": "^0.1.3",
    "aws-sdk": "^2.874.0",
    "aws-ssm": "^0.3.13",
    "babylon": "^7.0.0-beta.47",
    "bestzip": "^2.2.0",
    "chalk": "^4.0.0",
    "chokidar": "^3.5.1",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "common-types": "^1.20.4",
    "date-fns": "^2.19.0",
    "fast-glob": "^3.2.2",
    "globby": "^11.0.0",
    "inquirer": "^6.5.1",
    "js-yaml": "^4.0.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "matcher": "^4.0.0",
    "native-dash": "^1.11.0",
    "play-sound": "^1.1.3",
    "recast": "^0.20.4",
    "simple-git": "^2.12.0",
    "table": "^6.0.8",
    "typed-mapper": "^0.4.1",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/chai": "^4.2.15",
    "@types/chalk": "^2.2.0",
    "@types/globby": "^9.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14",
    "@types/table": "^6.0.0",
    "@types/webpack": "^4.41.27",
    "chai": "^4.2.0",
    "madge": "^4.0.2",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.2"
  }
}
