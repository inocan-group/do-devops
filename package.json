{
  "name": "dd",
  "version": "0.20.0",
  "description": "Devops commands for your nodejs and serverless projects",
  "keywords": [
    "devops",
    "nodejs",
    "serverless",
    "aws"
  ],
  "homepage": "https://github.com/inocan-group/devops#readme",
  "bugs": {
    "url": "https://github.com/inocan-group/devops/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inocan-group/devops.git"
  },
  "license": "MIT",
  "author": "Ken Snyder",
  "exports": {
    "*": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "bin": {
    "dd": "./bin/dd.js",
    "do": "./bin/dd.js"
  },
  "files": [
    "bin",
    "templates",
    "dist"
  ],
  "scripts": {
    "build": "run-s clean build:bundle",
    "build:bin": "yarn tsc -p tsconfig.build.json && chmod +x bin/dd.js",
    "build:bundle": "run-p build:bin build:dist",
    "build:dist": "yarn tsup src/shared/index.ts --dts --format='cjs,esm'",
    "circular": "yarn madge src --extensions ts --circular",
    "clean": "rimraf src/**/*.js dist/**/* bin/**/*",
    "dd": "./bin/dd.js",
    "leaves": "yarn madge src --extensions ts --leaves",
    "lint": "run-s lint:*",
    "lint:src": "yarn eslint src/**/*.ts --fix ",
    "lint:test": "yarn eslint test/**/*.ts --fix",
    "lint:tsc": "yarn tsc --noEmit",
    "orphans": "yarn madge src --extensions ts --orphans",
    "prepare": "ts-patch install -s",
    "prepublishOnly": "run-s lint build",
    "summary": "yarn madge src --extensions ts --summary",
    "test": "jest",
    "visualize": "yarn madge src --extensions ts -i /tmp/dep-ts-visualize.png && open /tmp/dep-ts-visualize.png",
    "visualize:js": "yarn madge bin --extensions js -i /tmp/dep-js-visualize.png && open /tmp/dep-js-visualize.png",
    "warnings": "yarn madge src --extensions ts --warnings",
    "watch": "run-p watch:*",
    "watch:build": "tsc -p tsconfig.build.json -w"
  },
  "dependencies": {
    "ast-types": "^0.14.2",
    "async-shelljs": "^0.1.4",
    "aws-sdk": "^2.1139.0",
    "aws-ssm": "^0.4.0",
    "bestzip": "^2.2.1",
    "callsites": "^3.1.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "common-types": "^1.31.1",
    "date-fns": "^2.28.0",
    "deepmerge": "^4.2.2",
    "destr": "^1.1.1",
    "diff": "^5.0.0",
    "exiftool-vendored": "^16.4.0",
    "find-up": "^5.0.0",
    "globby": "^11.1.0",
    "inferred-types": "^0.18.4",
    "inquirer": "^8.2.4",
    "js-yaml": "^4.1.0",
    "madge": "^5.0.1",
    "matcher": "^4.0.0",
    "native-dash": "^1.22.1",
    "play-sound": "^1.1.5",
    "recast": "^0.20.5",
    "sharp": "^0.30.4",
    "shelljs": "^0.8.5",
    "simple-git": "^2.48.0",
    "smartwrap": "^2.0.2",
    "table": "^6.8.0",
    "ts-morph": "^13.0.3",
    "typed-mapper": "^0.4.2",
    "xxhash-wasm": "^0.4.2"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/diff": "^5.0.2",
    "@types/exif": "^0.6.3",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^14.18.18",
    "@types/sharp": "^0.30.2",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "exiftool-vendored": "^15.12.1",
    "jest": "^27.5.1",
    "jest-extended": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.7.0",
    "ts-patch": "^2.0.1",
    "tsup": "^5.12.8",
    "typescript": "^4.6.4",
    "typescript-transform-paths": "^3.3.1",
    "vue": "^3.2.34"
  }
}