{
  "name": "dd",
  "version": "0.15.3",
  "description": "Devops commands for your nodejs and serverless projects",
  "keywords": [
    "devops",
    "nodejs",
    "serverless",
    "aws"
  ],
  "homepage": "https://github.com/inocan-group/devops#readme",
  "bugs": {
    "url": "https://github.com/inocan-group/devops/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inocan-group/devops.git"
  },
  "license": "MIT",
  "author": "Ken Snyder",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    "*": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "bin": {
    "dd": "./bin/dd.js",
    "do": "./bin/dd.js",
    "edd": "./bin/dd.mjs"
  },
  "files": [
    "bin",
    "templates",
    "dist"
  ],
  "scripts": {
    "prepare": "ts-patch install -s",
    "build": "run-s clean build:bundle",
    "build:bundle": "run-p build:bin build:dist",
    "build:bin": "yarn tsc -p tsconfig.build.json && chmod +x bin/dd.js",
    "build:dist": "yarn tsup src/shared/index.ts --dts --format='cjs,esm'",
    "circular": "yarn madge src --extensions ts --circular",
    "clean": "rimraf src/**/*.js dist/**/* bin/**/*",
    "dd": "./bin/dd.js",
    "prepublishOnly": "run-s lint build",
    "leaves": "yarn madge src --extensions ts --leaves",
    "lint": "run-s lint:*",
    "lint:src": "yarn eslint src/**/*.ts --fix ",
    "lint:test": "yarn eslint test/**/*.ts --fix",
    "lint:tsc": "yarn tsc --noEmit",
    "orphans": "yarn madge src --extensions ts --orphans",
    "summary": "yarn madge src --extensions ts --summary",
    "test": "jest",
    "visualize": "yarn madge src --extensions ts -i /tmp/dep-ts-visualize.png && open /tmp/dep-ts-visualize.png",
    "visualize:js": "yarn madge bin --extensions js -i /tmp/dep-js-visualize.png && open /tmp/dep-js-visualize.png",
    "warnings": "yarn madge src --extensions ts --warnings",
    "watch": "run-p watch:*",
    "watch:autoindex": "yarn dd autoindex -w",
    "watch:build": "tsc -p tsconfig.build.json -w"
  },
  "dependencies": {
    "ast-types": "^0.14.2",
    "async-shelljs": "^0.1.3",
    "aws-sdk": "2",
    "aws-ssm": "^0.4.0",
    "bestzip": "^2.2.0",
    "callsites": "^3.0.0",
    "chalk": "4",
    "chokidar": "^3.5.1",
    "command-line-args": "^5.2.0",
    "command-line-usage": "^6.1.1",
    "common-types": "^1.29.0",
    "date-fns": "^2.23.0",
    "deepmerge": "^4.2.2",
    "destr": "^1.1.0",
    "diff": "^5.0.0",
    "exiftool-vendored": "^15.3.0",
    "find-up": "^5.0.0",
    "globby": "^11.0.4",
    "inferred-types": "^0.16.0",
    "inquirer": "8.2.0",
    "js-yaml": "^4.1.0",
    "matcher": "^4.0.0",
    "native-dash": "^1.18.4",
    "play-sound": "^1.1.3",
    "pnpm": "^6.14.7",
    "recast": "^0.20.4",
    "sharp": "^0.29.1",
    "shift-parser": "^7.0.3",
    "simple-git": "^2.45.1",
    "smartwrap": "^2.0.2",
    "table": "^6.6.0",
    "ts-morph": "^13.0.2",
    "typed-mapper": "^0.4.1",
    "webpack": "^4.46.0"
  },
  "peerDependencies": {
    "madge": "5.0.1"
  },
  "devDependencies": {
    "@types/aws-sdk": "2",
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/diff": "^5.0.0",
    "@types/exif": "^0.6.2",
    "@types/inquirer": "^8.1.1",
    "@types/jest": "^27.0.1",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^14",
    "@types/sharp": "^0.29.0",
    "@types/webpack": "^4.41.27",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "jest": "^27.1.1",
    "jest-extended": "^1.2.0",
    "madge": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-patch": "^2.0.1",
    "tsup": "^5.10.1",
    "typescript": "^4.6.0-dev.20211205",
    "typescript-transform-paths": "3.3.1"
  }
}
