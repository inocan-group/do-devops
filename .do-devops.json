{
  "kind": "project",
  "projectConfig": true,
  "general": {
    "pkgManager": "pnpm"
  },
  "aws": {},
  "image": {
    "sourceDir": "test/data",
    "destinationDir": "temp",
    "metaDetail": "categorical",
    "rules": [
      {
        "name": "testing",
        "source": "test/data",
        "destination": "temp",
        "metaDetail": "categorical",
        "glob": "*.jpg",
        "widths": [640, 768, 1024, 1280, 1536],
        "preBlur": true,
        "sidecarDetail": "none"
      }
    ],
    "defaultWidths": [640, 768, 1024, 1280, 1536],
    "formatOptions": {
      "jpg": {
        "mozjpeg": true,
        "quality": 60
      },
      "avif": {
        "quality": 30
      },
      "webp": {
        "quality": 40
      }
    },
    "sidecar": "none"
  }
}
